{
  "name": "nodepy-pm",
  "version": "2.0.0-dev",
  "license": "MIT",
  "description": "The Node.py Package Manager",
  "repository": "https://github.com/nodepy/nodepy-package-manager",
  "keywords": ["nodepy", "package manager"],
  "categories": ["CLI"],
  "authors": ["rosensteinniklas@gmail.com"],
  "engines": [
    "nodepy>=2.0.0",
    "python>=2.7.0 || >=3.3.0"
  ],
  "include": [
    "lib/**",
    "scripts/**",
    "index.py",
    "LICENSE.txt",
    "README.md"
  ],
  "bin": {
    "nodepypm${py}": "index.py"
  },
  "pip_dependencies": {
    "appdirs": ">=1.4.2",
    "click": ">=6.7",
    "distlib": ">=0.2.4",
    "hammock": ">=0.2.4",
    "requests": ">=2.13.0",
    "six": ">=1.11.0"
  },
  "cfg(dev).dependencies": {
    "nodepy-nose": "git+https://github.com/nodepy/nodepy-nose.git@0.0.4"
  }
}
